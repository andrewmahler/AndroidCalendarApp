<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andrew.calendarapp">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name ="com.example.andrew.calendarapp.CreateActivity">
        </activity >
        <activity
            android:name ="com.example.andrew.calendarapp.DeleteActivity">
        </activity >
        <activity
            android:name ="com.example.andrew.calendarapp.DeleteActivity2">
        </activity >
        <activity
            android:name ="com.example.andrew.calendarapp.MoveActivity">
        </activity >
        <activity
            android:name ="com.example.andrew.calendarapp.MoveActivity2">
        </activity >
        <activity
            android:name ="com.example.andrew.calendarapp.View_Edit_Activity">
        </activity >
        <activity
            android:name ="com.example.andrew.calendarapp.Edit">
        </activity >
        <activity
            android:name ="com.example.andrew.calendarapp.Search">
        </activity >
        <activity
            android:name ="com.example.andrew.calendarapp.SearchDisplay">
        </activity >
    </application>

</manifest>